# UFOBeep Flutter App Environment Variables
# Copy to .env and fill in your values

# === API Configuration ===
API_BASE_URL=http://10.0.2.2:8000
API_VERSION=v1

# === Matrix Chat Configuration ===
MATRIX_BASE_URL=http://10.0.2.2:8008
MATRIX_SERVER_NAME=localhost

# === Firebase Configuration ===
# These will be in google-services.json and GoogleService-Info.plist
# FIREBASE_PROJECT_ID=ufobeep-project
# FIREBASE_APP_ID=your_firebase_app_id

# === Development Settings ===
DEBUG_MODE=true
MOCK_LOCATION=false
ENABLE_LOGGING=true

# === App Configuration ===
APP_NAME=UFOBeep
APP_VERSION=0.1.0
MIN_SDK_VERSION=21
TARGET_SDK_VERSION=34

# === Feature Flags ===
ENABLE_AR_COMPASS=true
ENABLE_PILOT_MODE=true
ENABLE_ANALYTICS=false

# === Internationalization ===
DEFAULT_LOCALE=en
SUPPORTED_LOCALES=en,es,de